{
  "name": "comquer/enum",
  "description": "Enumeration library",
  "keywords": [
    "enum",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dawid <krac> Kraczkowski",
      "email": "dawid.kraczkowski@gmail.com"
    },
    {
      "name": "Jakub Giminski",
      "email": "jakub@gigabyte.software"
    }
  ],
  "require": {
    "php": ">=7.2.0"
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit --coverage-text",
    "phpcs": "vendor/bin/phpcs --standard=PSR12 --warning-severity=0 src",
    "phpcsf": "vendor/bin/phpcbf --standard=PSR12 --warning-severity=0 src"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": ">=3.0"
  },
  "autoload": {
    "psr-4": {
      "Comquer\\Enum": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Comquer\\EnumTest\\": "tests/"
    }
  }
}
